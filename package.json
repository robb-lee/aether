{
  "name": "aether",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered website builder that creates professional websites in 30 seconds",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "dotenv -e .env.local -- turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "test:unit": "turbo test:unit",
    "test:e2e": "turbo test:e2e",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\"",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "task:complete": "node scripts/task-manager.js complete",
    "task:start": "node scripts/task-manager.js start",
    "task:status": "node scripts/task-manager.js status",
    "progress:update": "node scripts/update-progress.js",
    "daily:summary": "node scripts/daily-summary.js",
    "task": "node scripts/task-manager.js",
    "cleanup": "node scripts/cleanup.js",
    "cleanup:turbo": "node scripts/cleanup.js turbo",
    "cleanup:full": "node scripts/cleanup.js && pnpm install"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.3.3",
    "turbo": "^2.0.0",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.7.0",
  "keywords": [
    "ai",
    "website-builder",
    "nextjs",
    "no-code"
  ],
  "author": "Aether Team",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.1",
    "lucide-react": "^0.542.0"
  }
}
