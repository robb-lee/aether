{
  "name": "@aether/component-registry",
  "version": "0.1.0",
  "description": "Extensible component registry for AI-powered website generation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.{ts,tsx}",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.45.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./client-registry": {
      "types": "./dist/client-registry.d.ts",
      "default": "./dist/client-registry.js"
    },
    "./metadata": {
      "types": "./dist/metadata/index.d.ts",
      "default": "./dist/metadata/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./design-system": {
      "types": "./dist/design-system.d.ts",
      "default": "./dist/design-system.js"
    },
    "./design-kits": {
      "types": "./dist/design-system.d.ts",
      "default": "./dist/design-system.js"
    }
  },
  "files": [
    "dist",
    "metadata"
  ]
}