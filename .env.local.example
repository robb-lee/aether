# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# LiteLLM Configuration (Unified AI Gateway)
LITELLM_API_BASE=your_litellm_endpoint  # e.g., http://localhost:4000 or https://your-litellm-proxy.com
LITELLM_API_KEY=your_litellm_api_key

# Model Selection
AI_PRIMARY_MODEL=gpt-4-turbo-preview  # or claude-3-opus, etc.
AI_FALLBACK_MODEL=claude-3-haiku
AI_IMAGE_MODEL=dall-e-3

# Optional: Direct API keys (fallback if LiteLLM is down)
# OPENAI_API_KEY=your_openai_api_key_for_fallback
# ANTHROPIC_API_KEY=your_anthropic_api_key_for_fallback

# Vercel Configuration (for deployments)
VERCEL_TOKEN=your_vercel_token
VERCEL_TEAM_ID=your_vercel_team_id

# Redis Configuration (optional, for caching)
REDIS_URL=your_redis_url

# Stripe Configuration (for payments)
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_SITE_NAME=Aether
NEXT_PUBLIC_SITE_DESCRIPTION=AI-powered website builder that creates professional websites in 30 seconds